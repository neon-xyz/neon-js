{
  "name": "@neonpay/js",
  "version": "2.3.0",
  "description": "Neon.js loading utility",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./types/index.d.ts"
    }
  },
  "files": [
    "/dist",
    "/types"
  ],
  "scripts": {
    "check": "tsc --noEmit",
    "build": "tsup src/index.ts --format cjs,esm --clean",
    "watch": "npm run build -- --watch src",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neon-xyz/neon-js.git"
  },
  "keywords": [
    "neonpay",
    "Neon.js"
  ],
  "author": "Neon <devsupport@neonpay.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neon-xyz/neon-js/issues"
  },
  "homepage": "https://github.com/neon-xyz/neon-js/tree/main/packages/js#readme",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
